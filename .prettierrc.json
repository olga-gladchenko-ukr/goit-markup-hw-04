{
    "tabWidth": 2,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "avoid",
    "htmlWhitespaceSensitivity": "ignore",
    "proseWrap": "always",
    "embeddedLanguageFormatting": "auto",
    "singleAttributePerLine": false,
    "htmlSelfClosing": "never"
}